{
  "name": "BookmarkIt",
  "version": "1.0",
  "description": "Bookmark all the open tabs of the active window into organised bookmark folder.",
  "action": {
    "default_title": "BookmarkIt",
    "default_icon": {
      "16": "images/logo.png",
      "32": "images/logo.png",
      "48": "images/logo.png",
      "128": "images/logo.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": ["bookmarks", "tabs"],
  "manifest_version": 3
}
